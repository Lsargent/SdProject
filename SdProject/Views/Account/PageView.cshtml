@using SdProject.Models
@model SdProject.Models.AccountModels.DisplayAccountModel


@{
    ViewBag.Title = "PageView";
}

<h2>Welcome, @Model.User.UserName!</h2>


@if(Model.Houses.Count() == 0)
{
    <p>Tell Us about your house!</p>
    @Html.ActionLink("Add your house!", "EnterInfo", "House")
}

 @foreach(HouseDisplayModel house in Model.Houses) {
        @Html.Partial("~/Views/House/_House.cshtml", house)
    }


@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}